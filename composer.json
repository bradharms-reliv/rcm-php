{
  "name": "rcm/core",
  "description": "Reliv Content Management System",
  "type": "library",
  "license": "ISC",
  "keywords": [
    "reliv",
    "content",
    "manager",
    "module",
    "zf2"
  ],
  "homepage": "http://www.reliv.com/",
  "suggest": {
    "rcm/core-js": "Provides basic JS including jQuery, AngularJS and Bootstrap",
    "bower-asset/angular": "~1",
    "bower-asset/angular-utils-pagination": "~0.9",
    "bower-asset/rcm-loading": ">=0.1.14",
    "bower-asset/rcm-html-editor": ">=0.4.0",
    "bower-asset/rcm-dialog": ">=0.2.0"
  },
  "require": {
    "php": ">=7",
    "container-interop/container-interop": "^1",
    "doctrine/doctrine-orm-module": "1.*",
    "http-interop/http-middleware": "~0.4",
    "psr/http-message": "^1.0",
    "rcm/user": "~1.5",
    "rcm/api-lib": "~0.11",
    "rcm/user-ui": "1.*",
    "reliv/pipe-rat": "~0.5",
    "reliv/white-rat": "^0.2.0",
    "reliv/zf-config-factories": "3.* || 4.*",
    "rwoverdijk/assetmanager": "1.*",
    "zendframework/zend-cache": "2.*",
    "zendframework/zend-console": "2.*",
    "zendframework/zend-diactoros": "1.*",
    "zendframework/zend-eventmanager": "2.*",
    "zendframework/zend-filter": "2.*",
    "zendframework/zend-http": "2.*",
    "zendframework/zend-json": "2.*",
    "zendframework/zend-log": "2.*",
    "zendframework/zend-mvc": "2.*",
    "zendframework/zend-servicemanager": "2.*",
    "zendframework/zend-session": "2.*",
    "zendframework/zend-stdlib": "2.*",
    "zendframework/zend-validator": "2.*",
    "zendframework/zend-view": "2.*",
    "jakub-onderka/php-parallel-lint": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.27",
    "mockery/mockery": "^1.2.0",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Rcm\\ImmutableHistory\\": "immutable-history/src/",
      "Rcm\\ImmutableHistory\\Test\\": "immutable-history/test/",
      "Rcm\\SiteSettingsSections\\": "site-settings-sections/src/",
      "RcmAdmin\\": "admin/src/",
      "Rcm\\": "core/src/",
      "Rcm\\HttpLib\\": "http-lib/src/",
      "Rcm\\Acl\\": "acl/src",
      "Rcm\\RequestContext\\": "request-context/src",
      "Rcm\\CurrentRequest\\": "current-request/src",
      "RcmUser\\Ui\\": "user-ui/src/",
      "RcmUser\\": "user/src/"
    }
  }
}
